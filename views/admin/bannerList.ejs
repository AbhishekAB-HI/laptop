        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Banner List</h2>
                 
                </div>
              
            </div>
            <div class="card mb-4">
                <header class="card-header">
               
                </header> <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th scope="col">No</th>
                                    <th scope="col">BannerImages</th>
                                    <th scope="col">Bannername</th>
                                    <th scope="col">Banner title</th>
                                    <th scope="col">Description</th>
                                    <th scope="col">Delete</th>
                                    <th scope="col">Edit</th>
                                    <th scope="col">Block/Unblock</th>
                                   
                            
                                </tr>
                            </thead>
                            <tbody>
                                <%
                                if(baners.length > 0){
                                  baners.forEach((baner,i)=>{
                                    %>
                                    <tr>
                                      <th scope="row"><%=i+1%></th>
                                      <td><img src="/productImages/<%=baner.images[0]%>" width="80px"
                                        height="80px"></td>
                                      <td><%=baner.bannername%></td>
                                      <td><%=baner.bannertitle%></td>
                                      <td><%=baner.description%></td>
                                      <td>
                                        <a href="/admin/delete-banner?id=<%=baner._id%>">Delete</a>
                                      </td>
                                      <td>
                                        <a href="/admin/edit-banner?id=<%=baner._id%>">edit</a>                              
                                      </td>
                                      
                                      <td>                              
                                      <%
                                        if(baner.is_Blocked==false){
                                          %>
                                          <p>Unblocked</p>
                                          <%
                                        }else{
                                          %>
                                          <p>Blocked</p>
                                          <%
                                        }
                                      %>       
                                     </td>
                                   
                                    </tr>
                                    
                                    <%
                                  })
                                } else{
          
                                  %>
                                  <tr>
                                      <td colspan="5">Banner not found</td>
                                  </tr>
                                  <%
          
                                }
          
          
                                 %>
                                
                                
                              </tbody>
                          
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->

            
    
